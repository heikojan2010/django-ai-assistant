#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/coveralls@3.1.1/node_modules/coveralls/bin/node_modules:/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/coveralls@3.1.1/node_modules/coveralls/node_modules:/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/coveralls@3.1.1/node_modules:/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/coveralls@3.1.1/node_modules/coveralls/bin/node_modules:/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/coveralls@3.1.1/node_modules/coveralls/node_modules:/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/coveralls@3.1.1/node_modules:/Users/leviieathan/gaysthtcode/django-ai-assistant/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../coveralls/bin/coveralls.js" "$@"
else
  exec node  "$basedir/../coveralls/bin/coveralls.js" "$@"
fi
